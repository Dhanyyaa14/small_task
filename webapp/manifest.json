{
    "_version": "1.12.0",
    "sap.app": {
        "id": "my.app",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "SAP UI5 App",
        "description": "A simple SAP UI5 app"
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "my.app.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "models": {
            "salesModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            }
        }
    }
}
